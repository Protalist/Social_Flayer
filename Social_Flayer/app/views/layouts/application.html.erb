<!DOCTYPE html>
<html>
  <head>
    <title>SocialFlayer</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>

    <body onload="autocomplete()">

    <% if flash[:notice] %>
      <div class="notice"><%= flash[:notice] %></div>
    <% end %>
    <% if flash[:alert] %>
      <div class="aller"><%= flash[:alert] %></div>
    <% end %>
    <% if user_signed_in? %>
      <%= render 'shared/back' %>
      <%= render 'shared/change' %>
    <% end %>
    <%= yield %>
    <% if user_signed_in? %>
    
     Logged in as <%= current_user.email%> with role: <%= current_user.roles_mask %> [<%= link_to 'Edit', edit_user_registration_path%>,<%= link_to 'Logout',  destroy_user_session_path, :method => :delete%>]
    <% end %>
  </body>
</html>
